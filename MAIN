#include <iostream>
#include <stdio.h>

int main(){
	int a, b, x, y, m, n, cont, cont2;
	char A[2][2], B[2][2], R[2][2];
	
		printf("Dame los valores de la matriz A\n");
		for(a=0; a<2; a++){
			for(b=0;  b<2; b++){
				printf("Valores de A[%d][%d]:\n", a, b);
				scanf ("%d", &A[a][b]);
			}
		}
		
		for(a=0; a<2; a++){
			for(b=0; b<2; b++){
				printf("%d", A[a][b]);
			}
		}
		
		printf("Dame los valores de la matriz B:\n");
		for(x=0; x<2; x++){
			for(y=0;  y<2; y++){
				printf("Valores de B[%d][%d]\n", x, y);
				scanf ("%d", &B[x][y]);
			}
		}		
}	
